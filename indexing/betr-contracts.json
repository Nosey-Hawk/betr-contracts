{
  "version": "1",
  "name": "betr-contracts",
  "abis": {
    "betrStaking": {
      "path": "./betr-staking.abi.json"
    }
  },
  "instances": [
    {
      "abi": "betrStaking",
      "address": "0x808a12766632b456a74834F2FA8aE06DFC7482f1",
      "startBlock": 33842447,
      "chain": "base",
      "enrich": {
        "handlers": {
          "Staked(indexed address,indexed uint256)": {
            "calls": {
              "stakedAmount": {
                "name": "stakedAmount",
                "params": "event.params.staker",
                "required": true
              }
            },
            "entities": {
              "StakerBalance": {
                "staker address": "event.params.staker.toHexString()",
                "balance uint256": "calls.stakedAmount"
              }
            }
          },
          "Unstaked(indexed address,indexed uint256)": {
            "calls": {
              "stakedAmount": {
                "name": "stakedAmount",
                "params": "event.params.staker",
                "required": true
              }
            },
            "entities": {
              "StakerBalance": {
                "staker address": "event.params.staker.toHexString()",
                "balance uint256": "calls.stakedAmount"
              }
            }
          }
        }
      }
    }
  ]
}